	    <put-attribute name="default-center-right" value="/component/spider/matcherConfig/matcherConfig_update.jsp"></put-attribute>
	</definition>
	
	<!-- MatcherConfig 明细页 -->
	<definition name="spider.matcherConfig.detail" extends="default.manager">
	    <put-attribute name="default-center" value="spider.matcherConfig.detail.center"></put-attribute>
	</definition>
	<definition name="spider.matcherConfig.detail.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/spider/matcherConfig/matcherConfig_detail.jsp"></put-attribute>
