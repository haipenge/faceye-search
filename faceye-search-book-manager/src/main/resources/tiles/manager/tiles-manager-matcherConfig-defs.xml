    <!-- 作者:haipege -->
    <!-- MatcherConfig 管理 首页(列表页) -->
	<definition name="spider.matcherConfig.manager" extends="default.manager">
	    <put-attribute name="default-center" value="spider.matcherConfig.center"></put-attribute>
	</definition>
	<definition name="spider.matcherConfig.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/spider/matcherConfig/matcherConfig_center.jsp"></put-attribute>
	</definition>
	
	<!-- MatcherConfig 新增,编辑页面 -->
	<definition name="spider.matcherConfig.update" extends="default.manager">
