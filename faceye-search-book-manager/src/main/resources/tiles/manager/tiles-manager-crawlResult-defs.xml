<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
    <!-- 模块:spider 业务对像:CrawlResult 管理后台 tiles模板配置 -->
    <!-- 作者:haipege -->
    <!-- CrawlResult 管理 首页(列表页) -->
	<definition name="spider.crawlResult.manager" extends="default.manager">
	    <put-attribute name="default-center" value="spider.crawlResult.center"></put-attribute>
	</definition>
	<definition name="spider.crawlResult.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/spider/crawlResult/crawlResult_center.jsp"></put-attribute>
	</definition>
	
	<!-- CrawlResult 新增,编辑页面 -->
	<definition name="spider.crawlResult.update" extends="default.manager">
	    <put-attribute name="default-center" value="spider.crawlResult.update.center"></put-attribute>
	</definition>
	<definition name="spider.crawlResult.update.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/spider/crawlResult/crawlResult_update.jsp"></put-attribute>
	</definition>
	
	<!-- CrawlResult 明细页 -->
	<definition name="spider.crawlResult.detail" extends="default.manager">
	    <put-attribute name="default-center" value="spider.crawlResult.detail.center"></put-attribute>
	</definition>
	<definition name="spider.crawlResult.detail.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/spider/crawlResult/crawlResult_detail.jsp"></put-attribute>
	</definition>
	
</tiles-definitions>