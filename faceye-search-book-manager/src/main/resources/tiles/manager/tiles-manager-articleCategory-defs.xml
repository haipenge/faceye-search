	</definition>
	<definition name="search.articleCategory.update.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/search/articleCategory/articleCategory_update.jsp"></put-attribute>
	</definition>
	
	<!-- ArticleCategory 明细页 -->
	<definition name="search.articleCategory.detail" extends="default.manager">
	    <put-attribute name="default-center" value="search.articleCategory.detail.center"></put-attribute>
	</definition>
	<definition name="search.articleCategory.detail.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/search/articleCategory/articleCategory_detail.jsp"></put-attribute>
