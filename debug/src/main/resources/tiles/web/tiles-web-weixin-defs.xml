    <!-- Weixin 管理 首页(列表页) -->
    <definition name="weixin.web" template="/component/weixin/template/default.jsp">
        <put-attribute name="default-center" value="/component/weixin/template/default_center.jsp"></put-attribute>
        <put-attribute name="default-footer" value="/component/weixin/template/default_footer.jsp"></put-attribute>
    </definition>
	<definition name="weixin.weixin.home" extends="weixin.web">
	    <put-attribute name="default-center" value="/component/weixin/weixin/weixin_center.jsp"></put-attribute>
	</definition>
	
	<!-- Weixin 明细页 -->
	<definition name="weixin.weixin.detail" extends="weixin.web">
	    <put-attribute name="default-center" value="/component/weixin/weixin/weixin_detail.jsp"></put-attribute>
	</definition>
	
	<!-- qq 明细页 -->
	<definition name="weixin.qq.detail" extends="weixin.web">
	    <put-attribute name="default-center" value="/component/weixin/weixin/qq_detail.jsp"></put-attribute>
	</definition>
	
