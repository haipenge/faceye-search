<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
    <!-- 模块:search 业务对像:Movie 管理后台 tiles模板配置 -->
    <!-- 作者:haipege -->
    <!-- Movie 管理 首页(列表页) -->
	<definition name="search.movie.manager" extends="default.manager">
	    <put-attribute name="default-center" value="search.movie.center"></put-attribute>
	</definition>
	<definition name="search.movie.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/search/movie/movie_center.jsp"></put-attribute>
	</definition>
	
	<!-- Movie 新增,编辑页面 -->
	<definition name="search.movie.update" extends="default.manager">
	    <put-attribute name="default-center" value="search.movie.update.center"></put-attribute>
	</definition>
	<definition name="search.movie.update.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/search/movie/movie_update.jsp"></put-attribute>
	</definition>
	
	<!-- Movie 明细页 -->
	<definition name="search.movie.detail" extends="default.manager">
	    <put-attribute name="default-center" value="search.movie.detail.center"></put-attribute>
	</definition>
	<definition name="search.movie.detail.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/search/movie/movie_detail.jsp"></put-attribute>
	</definition>
	
</tiles-definitions>