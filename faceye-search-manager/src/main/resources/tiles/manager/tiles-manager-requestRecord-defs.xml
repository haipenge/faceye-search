<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
    <!-- 模块:search 业务对像:RequestRecord 管理后台 tiles模板配置 -->
    <!-- 作者:haipege -->
    <!-- RequestRecord 管理 首页(列表页) -->
	<definition name="search.requestRecord.manager" extends="default.manager">
	    <put-attribute name="default-center" value="search.requestRecord.center"></put-attribute>
	</definition>
	<definition name="search.requestRecord.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/search/requestRecord/requestRecord_center.jsp"></put-attribute>
	</definition>
	
	<!-- RequestRecord 新增,编辑页面 -->
	<definition name="search.requestRecord.update" extends="default.manager">
	    <put-attribute name="default-center" value="search.requestRecord.update.center"></put-attribute>
	</definition>
	<definition name="search.requestRecord.update.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/search/requestRecord/requestRecord_update.jsp"></put-attribute>
	</definition>
	
	<!-- RequestRecord 明细页 -->
	<definition name="search.requestRecord.detail" extends="default.manager">
	    <put-attribute name="default-center" value="search.requestRecord.detail.center"></put-attribute>
	</definition>
	<definition name="search.requestRecord.detail.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/search/requestRecord/requestRecord_detail.jsp"></put-attribute>
	</definition>
	
</tiles-definitions>