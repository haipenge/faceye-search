<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
    <!-- 模块:search 业务对像:Subject 管理后台 tiles模板配置 -->
    <!-- 作者:haipege -->
    <!-- Subject 管理 首页(列表页) -->
	<definition name="search.subject.manager" extends="default.manager">
	    <put-attribute name="default-center" value="search.subject.center"></put-attribute>
	</definition>
	<definition name="search.subject.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/search/subject/subject_center.jsp"></put-attribute>
	</definition>
	
	<!-- Subject 新增,编辑页面 -->
	<definition name="search.subject.update" extends="default.manager">
	    <put-attribute name="default-center" value="search.subject.update.center"></put-attribute>
	</definition>
	<definition name="search.subject.update.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/search/subject/subject_update.jsp"></put-attribute>
	</definition>
	
	<!-- Subject 明细页 -->
	<definition name="search.subject.detail" extends="default.manager">
	    <put-attribute name="default-center" value="search.subject.detail.center"></put-attribute>
	</definition>
	<definition name="search.subject.detail.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/search/subject/subject_detail.jsp"></put-attribute>
	</definition>
	
</tiles-definitions>